<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>ServletEditSaveStudent</servlet-name>
        <servlet-class>servlets.ServletEditSaveStudent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletEditSaveStudent</servlet-name>
        <url-pattern>/editSave</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ServletDeleteStudent</servlet-name>
        <servlet-class>servlets.ServletDeleteStudent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletDeleteStudent</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ServletGetAddStudent</servlet-name>
        <servlet-class>servlets.ServletGetAddStudent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletGetAddStudent</servlet-name>
        <url-pattern>/getAdd</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletRegisterForm</servlet-name>
        <servlet-class>servlets.ServletRegisterForm</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletRegisterForm</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletSearch</servlet-name>
        <servlet-class>servlets.ServletSearch</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletSearch</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
</web-app>